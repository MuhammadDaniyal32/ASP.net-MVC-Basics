
@{
    ViewBag.Title = "Index";
}

<h1>INDEX</h1>
<div>
    @if(Model != null)
    {
        <ul class="list-group">
            <li class="list-group-item active">Employee Info</li>
            <li class="list-group-item">Employee Id:<b>@Model.Emp_id</b></li>
            <li class="list-group-item">Name:<b>@Model.Name</b></li>
            <li class="list-group-item">Address:<b>@Model.Address</b></li>
            <li class="list-group-item">Department Id:<b>@Model.Dep_id</b></li>
        </ul>
    }
</div>

